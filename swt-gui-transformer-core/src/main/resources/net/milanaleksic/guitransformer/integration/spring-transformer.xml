<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="sgtEmbeddingService" class="net.milanaleksic.guitransformer.converters.EmbeddingService" />

    <bean id="sgtResourceBundleProvider" class="net.milanaleksic.guitransformer.providers.impl.SimpleResourceBundleProvider"/>

    <bean id="sgtObjectProvider" class="net.milanaleksic.guitransformer.providers.impl.AlwaysReturnNullObjectProvider"/>

    <bean id="sgtImageProvider" class="net.milanaleksic.guitransformer.providers.impl.AlwaysReturnEmptyImageProvider"/>

    <bean class="net.milanaleksic.guitransformer.providers.impl.ConfigurableBuilderProvider" />

    <bean class="net.milanaleksic.guitransformer.providers.impl.ConfigurableConverterProvider"/>

    <bean class="net.milanaleksic.guitransformer.providers.impl.ConfigurableShortcutsProvider"/>

    <bean class="net.milanaleksic.guitransformer.integration.loader.impl.SpringLoader" />

    <bean class="net.milanaleksic.guitransformer.converters.OldSchoolObjectCreator" />

    <bean class="net.milanaleksic.guitransformer.converters.ShortHandObjectCreator" />

    <bean class="net.milanaleksic.guitransformer.converters.ObjectConverter" />

    <bean class="net.milanaleksic.guitransformer.converters.typed.IntegerConverter" />

    <bean id="transformer" class="net.milanaleksic.guitransformer.Transformer" />

    <bean id="nodeProcessor" class="net.milanaleksic.guitransformer.converters.ObjectCreationNodeProcessor" />

</beans>
